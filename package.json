{
  "name": "unit-tesing-mocha",
  "version": "1.0.0",
  "description": "unit testing using mocha and chai in node js",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js",
    "test": "mocha 'test-cases/**/*.spec.js'",
    "test-single": "mocha",
    "test-report": "mocha 'test-cases/**/*.spec.js' --reporter mochawesome --require mochawesome/register",
    "coverage": "nyc --reporter=lcov --reporter=test npm run test"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.26.0",
    "express": "^4.17.2",
    "mochawesome": "^7.1.0",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "mocha": "^9.2.0",
    "nock": "^13.2.4",
    "sinon": "^13.0.1"
  }
}
